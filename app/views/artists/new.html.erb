<h1> Create Artist </h1>

<% if flash[:my_errors] %>
    <%flash[:my_errors].each do |error|%>
       <p> <%= error %> </p>
    <% end%>
<% end %>


<%= form_for @artist do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name%>

    <%= f.label :age %>
    <%= f.number_field :age%>

    <%= f.label :title %>
    <%= f.text_field :title%>

    <%= f.submit "Create Artist"%>

<% end %>